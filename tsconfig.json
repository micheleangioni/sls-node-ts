{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2018"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "es2020",
    "outDir": ".webpack"
  },
  "exclude": ["node_modules", "test"]
}
